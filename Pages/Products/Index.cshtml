@page
@model SupermarketWEB.Pages.Products.IndexModel

@{
    ViewData["Title"] = "Index Products";
}

<h1>Index Products</h1>

<table class="table">
    <thead>
        <tr>
            <th>@Html.DisplayNameFor(model => model.Products[0].Id)</th>
            <th>@Html.DisplayNameFor(model => model.Products[0].Name)</th>
            <th>@Html.DisplayNameFor(model => model.Products[0].Price)</th>
            <th>@Html.DisplayNameFor(model => model.Products[0].Stock)</th>
            <th>@Html.DisplayNameFor(model => model.Products[0].CategoryId)</th>
            <th>@Html.DisplayNameFor(model => model.Products[0].Category)</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model.Products)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.Id)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Price)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stock)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Stock)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.CategoryId)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Category)
                </td>
                <td>
                </td>
            </tr>
        }
    </tbody>
</table>

<p>
    <a asp-page="Create" class=" btn btn-success">Create New</a>
</p>
